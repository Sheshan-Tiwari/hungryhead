<div class="container-fluid col-md-8 auto-margin">
    <div class="m-t-50" id="rootwizard">
        <ul class="nav nav-tabs nav-tabs-linetriangle nav-tabs-separator nav-stack-sm">
	    		<li>
	          <%= link_to(wizard_path(:hello)) do %>
	            <i class="fa fa-list-ul tab-icon"></i>
	            <span>Guidelines</span>
