<!-- MODAL STICK UP  -->
<div class="modal fade stick-up" id="messagePopupModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header clearfix text-left">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                <i class="pg-close fs-14"></i>
                </button>
                <h5>Send <span class="semi-bold b-b b-grey p-b-5"> a private message</span></h5>
            </div>
            <div class="modal-body">
                <%= render partial: 'form' %>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<!-- END MODAL STICK UP  -->