<div class="widget-16 panel no-border no-margin">
    <div class="panel-heading m-b-20">
        <div class="panel-title b-b b-grey p-b-5">
          <i class="fa fa-users text-danger"></i> Latest followers
        </div>
    </div>
    <div class="panel-body">
        <ul class="list-unstyled no-margin">
            <%= react_component('ListingWidget', {listing: @user.get_followers.take(7).map{|u| {name: u.name, name_badge: u.name_badge,  avatar: u.get_avatar,  url: profile_path(u)}}, count: @user.followers_counter.value, path: profile_followers_path(@user), type: "followers" }.to_json) %>
        </ul>
    </div>
</div>
