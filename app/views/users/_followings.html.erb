<div class="widget-16 panel no-border no-margin">
    <div class="panel-heading m-b-20">
        <div class="panel-title b-b b-grey p-b-5">
          <i class="fa fa-user-plus text-danger"></i> Latest followings
        </div>
    </div>
    <div class="panel-body">
    <ul class="list-unstyled no-margin">
        <%= react_component('ListingWidget', {listing: @user.get_followings.take(15).map{|u| {name: u.name, name_badge: u.name_badge, avatar: u.get_avatar,  url: profile_path(u)}}, count: @user.followings_counter.value, path: profile_followings_path(@user), type: 'followings' }.to_json) %>
      </ul>
    </div>
</div>
