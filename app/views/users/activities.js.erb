var $el = $('.profile-activity-feed');
$el.append("<%=j render partial: 'activities' %>");

ReactRailsUJS.mountComponents();
$('body textarea').on('focus', function(){
  $(this).autosize();
});
$('a.load-more-posts').attr('href', "<%= profile_activities_path(page: @activities.next_page) %>");
<% unless @activities.next_page %>
  $('.pagination-box').remove();
<% end %>