var $el = $('.profile-posts');
$el.append("<%=j render partial: 'activities' %>");

ReactRailsUJS.mountComponents();
$('body textarea').on('focus', function(){
  $(this).autosize();
});
$('a.load-more-posts').attr('href', "<%= profile_posts_path(page: @posts.next_page) %>");
<% unless @posts.next_page %>
  $('.pagination-box').remove();
<% end %>