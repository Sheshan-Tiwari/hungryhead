<%= cache(['beta', 'page', 'v1']) do %>
<div class="learn-wrapper bg-white">
  <div class="learn" data-pages="social">
    <div class="jumbotron no-margin" data-pages="parallax" data-social="cover">
        <div class="how-photo">
        <div class="container vmiddle">
        <div class="col-md-4 p-t-40">
          <div class="hero-section inner">
              <h1 class="text-white">
                How it works
              </h1>
              <p class="text-white">Understand the basic building blocks</p>
              <div class="btn-box m-t-20">
                <%= link_to('Get Started', get_started_path, class: "btn btn-ghost") %>
              </div>
          </div>
          </div>
      </div>
      </div>
    </div>
  </div>

  <div class="profile-nav displayblock overflow-hidden p-l-15">
    <div class="container text-center">
      <ul class="profile-name inline no-style p-t-15 p-b-15 no-margin">
        <li class="inline p-l-15 p-r-15">
          <a class="bold fs-14 font-opensans" href="#join">1. Join</a>
        </li>
        <li class="inline p-l-15 p-r-15">
          <a class="bold fs-14 font-opensans" href="#complete_your_profile">2. Complete your profile</a>
        </li>
        <li class="inline p-l-15 p-r-15">
          <a class="bold fs-14 font-opensans" href="#connect">3. Connect</a>
        </li>
        <li class="inline p-l-15 p-r-15">
          <a class="bold fs-14 font-opensans" href="#share_skills">4. Share skills</a>
        </li>
        <li class="inline p-l-15 p-r-15">
          <a class="bold fs-14 font-opensans" href="#pitch_idea">5. Pitch your idea</a>
        </li>
        <li class="inline p-l-15 p-r-15">
          <a class="bold fs-14 font-opensans" href="#collaboration">6. Collaborate</a>
        </li>
        <li class="inline p-l-15 p-r-15">
          <a class="bold fs-14 font-opensans" href="#measure">7. Measure and Validate</a>
        </li>
      </ul>
    </div>
  </div>

  <!-- BEGIN CONTENT SECTION -->
  <section class=" bg-master-lightest p-b-85 p-t-75 no-overflow" id="join">
    <div class="container">
      <div class="md-p-l-20 md-p-r-20 xs-no-padding">
        <div class="row">
          <div class="col-sm-5">
            <h5 class="block-title hint-text no-margin">Join hungryhead</h5>
            <h1 class="m-t-5 m-b-20">Create account.</h1>
            <p class="m-b-20">Menu clipping makes it easier to keep track of the page you are at & helps you to identify menu labels using abbreviations</p>
          </div>
          <div class="col-sm-6 col-sm-offset-1 xs-p-l-15 xs-p-r-15">
            <div class="content-mask-md">
              <%= image_tag('product/join.png', class: "image-responsive-height lazy b-lightgrey b-a box-shadow") %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- END CONTENT SECTION -->

  <!-- BEGIN CONTENT SECTION -->
  <section class="p-b-85 p-t-75 no-overflow" id="complete_your_profile">
    <div class="container">
      <div class="md-p-l-20 md-p-r-20 xs-no-padding">
        <div class="row">
          <div class="col-sm-4">
            <div class="content-mask-md">
              <%= image_tag('product/profile.png', class: "image-responsive-height lazy b-lightgrey b-a box-shadow") %>
            </div>
          </div>
          <div class="col-sm-6 col-sm-offset-2 xs-p-l-15 xs-p-r-15">
            <h5 class="block-title hint-text no-margin">Realtime Collaboration</h5>
            <h1 class="m-t-5 m-b-20">Easier to remember and access.</h1>
            <p class="m-b-20">Menu clipping makes it easier to keep track of the page you are at & helps you to identify menu labels using abbreviations</p>
            <p class="hint-text font-arial small-text col-md-8 no-padding">This is something extra to give users an exceptional user experience with a condensed menu.</p>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- END CONTENT SECTION -->

  <!-- BEGIN CONTENT SECTION -->
  <section class="bg-master-lightest p-b-85 p-t-75 no-overflow" id="connect">
