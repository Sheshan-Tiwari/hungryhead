<script type="text/javascript">
  $('[data-main-nav-click]').on('click', function() {
    _gs('event', $(this).data('main-nav-click'), {
      type: 'Visitor'
    });
  });
  $('[data-footer-nav-click]').on('click', function() {
    _gs('event', $(this).data('footer-nav-click'), {
      type: 'Visitor'
    });
  });
  $('[data-content-link-click]').on('click', function() {
    _gs('event', $(this).data('content-link-click'), {
      type: 'Visitor'
    });
  });
  <% if user_signed_in? and current_user  %>
    _gs('identify', {
      type: current_user.role.downcase,
      id: current_user.uid,
      email: current_user.email,
      name: current_user.name,
      avatar: current_user.get_avatar
    });
  <% end %>
</script>