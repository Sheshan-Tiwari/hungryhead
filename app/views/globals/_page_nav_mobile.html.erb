<!-- START HEADER -->
<div class="header">
    <div class="pull-left full-height visible-sm visible-xs">
      <div class="sm-action-bar">
        <a href="javascript;" class="btn-link text-white toggle-sidebar text-white" data-toggle="sidebar">
          <span class="fa fa-bars"></span>
        </a>
      </div>
    </div>
    <div class="container">
      <div class="sm-table">
        <div class="header-inner">
          <div class="brand inline">
          <%= link_to(root_path) do %>
            <img src="<%= image_path('hungryhead_white.png') %>" alt="logo" data-src="<%= image_path('hungryhead_white.png') %>" data-src-retina="<%= image_path('hungryhead_white.png') %>" width="30">
          <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
