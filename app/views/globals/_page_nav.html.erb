<!-- START HEADER -->
<div class="header bg-white b-b b-grey box-shadow">
<div class="container">
  <div class=" pull-left sm-table">
    <div class="header-inner">
      <div class="brand inline">
      <%= link_to(root_path) do %>
        <img src="<%= image_path('hungryhead.png') %>" alt="logo" data-src="<%= image_path('hungryhead.png') %>" data-src-retina="<%= image_path('hungryhead.png') %>" width="30">
      <% end %>
      </div>
     <ul class="pages-links fs-14 inline no-margin no-style m-l-20 p-l-30 p-r-20">
      <li class="p-r-15 fs-14 inline font-heading">
        <%= link_to(story_path) do %>
          <span class="text-master">Story</span>
        <% end %>
      </li>
      <li class="p-r-15 fs-14 inline font-heading">
        <%= link_to(how_it_works_path) do %>
          <span class="text-master">How it works? </span>
        <% end %>
      </li>
     </ul>
    </div>
  </div>

  <div class="pull-right">
    <div class="visible-lg visible-md m-t-10">
      <div class="pull-left p-t-5 font-heading">
        <div class="pull-right">
          <%= link_to(get_started_path, title: "Find more information", class: "btn btn-master bg-transparent inline") do %>
            <span class="text-master">Get Started</span>
          <% end %>
        </div>

        <div class="pull-right p-r-20">
          <%= link_to(help_path, title: "Questions?", class: "btn btn-master bg-transparent inline") do %>
            <span class="text-master">Questions?</span>
          <% end %>
        </div>

      </div>
    </div>
  </div>
  </div>
</div>