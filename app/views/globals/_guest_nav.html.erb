<!-- START HEADER -->
<div class="header">
<div class="container">
  <div class=" pull-left sm-table">
    <div class="header-inner">
      <div class="brand inline">
      <%= link_to(root_path) do %>
        <img src="<%= image_path('hungryhead_white.png') %>" alt="logo" data-src="<%= image_path('hungryhead_white.png') %>" data-src-retina="<%= image_path('hungryhead_white.png') %>" width="30">
      <% end %>
      </div>
      <ul class="pages-links fs-16 inline no-margin no-style m-l-20 p-l-30 p-r-20">
       <li class="p-r-15 fs-16 inline">
         <%= link_to(learn_path) do %>
           <span class="text-white">Learn</span>
         <% end %>
       </li>
       <li class="p-r-15 fs-16 inline">
         <%= link_to(story_path) do %>
           <span class="text-white">Story</span>
         <% end %>
       </li>
       <li class="fs-16 inline">
         <%= link_to(help_path) do %>
           <span class="text-white">Help</span>
         <% end %>
       </li>
      </ul>
    </div>
  </div>

  <div class=" pull-right">
    <div class="visible-lg visible-md m-t-10">
      <div class="pull-left p-r-20 p-t-10 fs-16 font-heading">
        <div class="dropdown pull-right">
          <%= link_to(get_started_path, title: "Click to Register") do %>
            <span class="text-white fs-16">Get Started</span>
          <% end %>
        </div>
      </div>
      <%= link_to(new_user_session_path, class: "pull-left p-t-10 fs-16 font-heading") do %>
        <span class="text-white">Login</span>
      <% end %>
    </div>
  </div>
  </div>
</div>